@page
@model RostrosFelices.Pages.Servicios.IndexModel
@{
}
<h1>REGISTRE SERVICIO</h1>
<p>
    <a asp-page="Create" class="btn btn-success">REGISTRESE AQUI</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Servicios[0].NombreDelServicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicios[0].Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicios[0].Cliente.NombreCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicios[0].Empleado.NombreEmpleado)
            </th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Servicios)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreDelServicio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fecha)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cliente.NombreCliente)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Empleado.NombreEmpleado)
                </td>
       
       





            </tr>
        }
    </tbody>
</table>
